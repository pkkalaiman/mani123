package com.xworkz.accountapp.account;

import com.xworkz.accountapp.BankAccount;

public class AccountTester {

	public static void main(String a[]) {
		
		BankAccount account = new BankAccount();
		account.deposite(5000);
		account.deposit(400);
		System.out.println("My Account Balance is"+account.balance);
		
		
		BankAccount friendsAccount = new BankAccount();
		friendsAccount.deposite(700);
		System.out.println("My Account Balance is"+account.balance);
		
		BankAccount account1 = new BankAccount();
		account1.deposite(400);
		System.out.println("Account Balance is deposit"+account1.balance);
		
		
		
		
		
		
		
		
		
	}
	
	
	
	
}
